<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.0.13</Version>
    <Authors>Sylveon</Authors>
    <Description>C# wrapper for the Martine API</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.0.13</AssemblyVersion>
    <FileVersion>1.0.13</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>martinebot.com</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Sylveon76/MartineApi.Net</RepositoryUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageVersion>1.0.13</PackageVersion>
    <TargetFrameworks>net5.0;</TargetFrameworks>
    <PackageId>MartineApiNet</PackageId>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\MartineApiNet.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Refit" Version="6.3.2" />
  </ItemGroup>

</Project>
